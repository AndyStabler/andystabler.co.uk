---
pageable: true
---
<% wrap_layout "blog/index" do %>
  <% page_articles.each do |article| %>
    <%= partial "layouts/blog/snippet", :locals => { :article => article } %>
  <% end %>
  <%= link_to "Older", next_page if !next_page.nil? %>
  <%= link_to "Newer", prev_page if !prev_page.nil? %>
<% end %>
